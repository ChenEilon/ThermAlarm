@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div id="addPersonForm">
    <form>
        <label>First Name:</label>
        <br />
        <input type="firstName" />
        <br />
        <label>Last Name:</label>
        <br />
        <input type="lastName" />
        <br />
        <label>Email:</label>
        <br />
        <input type="email" />
        <br />
        <label>BT id:</label>
        <br />
        <input type="BTid" />
        <br />

        <input type="submit" value="Add member" />
    </form>
</div>
